{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "target": "es6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/types/meeting",
    "middleware.ts_",
    "build/types/**/*.ts",
    ".nextdev/types/**/*.ts"
, "middleware.old-ts", "src/app/layout.tsx", "src/app/page.tsx", "src/app/[locale]/page.tsx.test", "src/app/[locale]/layout.tsx.test"  ],
  "exclude": [
    "node_modules"
  ]
}
